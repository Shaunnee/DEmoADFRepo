{
	"name": "dataflowRank",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "CustPay",
						"type": "DatasetReference"
					},
					"name": "sourcerank"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "outdataset",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "rank1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          {Customer name} as string,",
				"          Title as string,",
				"          depid as string,",
				"          salary as string,",
				"          department as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> sourcerank",
				"sourcerank rank(desc(salary, true),",
				"     output(rnk as long)) ~> rank1",
				"rank1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     partitionFileNames:[''],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     partitionBy('hash', 1)) ~> sink1"
			]
		}
	}
}