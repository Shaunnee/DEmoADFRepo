{
	"name": "dfAseertErr",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "datasetEmpErr",
						"type": "DatasetReference"
					},
					"name": "source1"
				},
				{
					"dataset": {
						"referenceName": "DEpt",
						"type": "DatasetReference"
					},
					"name": "department"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "assert1"
				},
				{
					"name": "derivedColumn1"
				},
				{
					"name": "goodRrows"
				},
				{
					"name": "badRows"
				}
			],
			"scriptLines": [
				"source(output(",
				"          ID as string,",
				"          {Customer name} as string,",
				"          Title as string,",
				"          gender as string,",
				"          depid as string,",
				"          doj as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source1",
				"source(output(",
				"          Id as string,",
				"          name as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> department",
				"source1, department assert(expectTrue(!isNull(toDate(doj, 'yyyyMMdd'))\r",
				", false, 'assertDOJ', null, 'check date doj is valid date'),",
				"     expectUnique(source1@ID, false, 'assertEmpID', null, 'expect unoque id'),",
				"     expectExists(depid == department@Id, false, 'assertDepId')) ~> assert1",
				"assert1 derive(isErrRow = isError(),",
				"          {no dep id} = hasError('assertDepId')) ~> derivedColumn1",
				"derivedColumn1 filter(isErrRow ==false()) ~> goodRrows",
				"derivedColumn1 filter(isErrRow ==true()) ~> badRows"
			]
		}
	}
}