{
	"name": "dataflowFunc",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "datsetCustDep",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "derivedColumn1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          ID as string,",
				"          {Customer name} as string,",
				"          Title as string,",
				"          depid as string,",
				"          country as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: true,",
				"     ignoreNoFilesFound: false) ~> source1",
				"source1 derive(country = iif(country=='uk',1, iif(country=='india',0,-01))) ~> derivedColumn1"
			]
		}
	}
}